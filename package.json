{
  "name": "agg-proj",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "bcryptjs-react": "^2.4.6",
    "browserify-zlib": "^0.2.0",
    "fs": "^0.0.1-security",
    "https-browserify": "^1.0.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "node": "^23.11.0",
    "node-modules": "^0.0.1",
    "nth-check": "^2.1.1",
    "playht": "^0.19.0",
    "postcss": "^8.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.2",
    "react-scripts": "^5.0.1",
    "stream-http": "^3.2.0",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": "warn",
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|webp|svg)$": "<rootDir>/src/MainComponents/__mocks__/images.js",
      "react-router-dom": "<rootDir>/node_modules/react-router-dom"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!axios)"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "axios": "^1.8.4",
    "buffer": "^6.0.3",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.1",
    "eslint-plugin-n": "^17.17.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jsdom": "^26.1.0",
    "react-router-dom": "^7.5.1",
    "text-encoding": "^0.7.0",
    "webpack": "^5.99.7",
    "webpack-cli": "^4.10.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "> 1%"
            ],
            "node": "current"
          }
        }
      ],
      "@babel/preset-react"
    ]
  }
}
